

def sum_earnings(input):
    try:
        num_of_nums = input.count(",") +1
        input_value = list(map(int, input.split(",")))
        if len(input_value) != num_of_nums:
            raise ValueError
        balance = 0
        for num in input_value:
            balance += num
            if balance < 0:
                balance = 0
        print(balance)
    except ValueError:
        print(0)
sum_earnings(input)
